{
    "version": 2,
    "builds": [
        {
            "src": "./highlander.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        
        { "handle": "filesystem" },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    
    ],
    "env": {
        "FLASK_ENV": "production",
        "FLASK_APP": "app.py"
      },
      "functions": {
        "api": {
          "runtime": "python3.8",
          "includeFiles": ["requirements.txt"],
          "memory": 1024
        }
      }
}